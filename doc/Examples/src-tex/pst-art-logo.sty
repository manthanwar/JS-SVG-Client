%===============================================================================
% Logo by Japan Lady in London for Malaysian Company
%-------------------------------------------------------------------------------
\newcommand{\psLogoJapanLady}[1][]{\setkeys{\kvf}{#1}%
\FPeval{\wide}{round(\iconWidth/20:3)}\psscalebox{\wide}{%
\psset{unit=1mm,dimen=middle,cornersize=absolute,linearc=0,framearc=0,linejoin=1,fillstyle=none}%
%
\FPeval{\sqrtA}{\fpeval{sqrt(8)}}%
\FPset{\angYaa}{-130.1457}% \FPset{\angA}{-130.14572660564244}%
\FPset{\angYab}{56.4059}% \FPset{\angB}{56.40593131395438}%
\FPset{\angYba}{-120.8726}% \FPset{\angYba}{-120.87260037810255}%
\FPset{\angYbb}{47.1328}% \FPset{\angYbb}{47.13280508641449}%
\FPset{\lineYaax}{4}%
\FPset{\lineYaay}{0}%
\FPset{\lineYabx}{31}%
\FPset{\lineYaby}{36}%
\FPeval{\lineYbax}{\fpeval{\lineYaax+\sqrtA}}%
\FPset{\lineYbay}{\lineYaay}%
\FPeval{\lineYbbx}{\fpeval{\lineYabx+\sqrtA}}%
\FPset{\lineYbby}{\lineYaby}%
\FPset{\angGaa}{-113.6569}% \FPset{\angGaa}{-113.65691728568544}%
\FPset{\angGab}{39.9171}% \FPset{\angGab}{39.917121993997384}%
\FPset{\angGba}{-103.9032}% \FPset{\angGba}{-103.90321222846676}%
\FPset{\angGbb}{30.1634}% \FPset{\angGbb}{30.163416936778685}%
\FPset{\lineGaax}{9}%
\FPset{\lineGaay}{0}%
\FPset{\lineGabx}{36}%
\FPset{\lineGaby}{36}%
\FPeval{\lineGbax}{\fpeval{\lineGaax+\sqrtA}}%
\FPset{\lineGbay}{\lineGaay}%
\FPeval{\lineGbbx}{\fpeval{\lineGabx+\sqrtA}}%
\FPset{\lineGbby}{\lineGaby}%
%
% drawCircleBgM
\psset{fillstyle=\fillStyleCircBg,fillcolor=\fillColorCircBg,linestyle=\lineStyleCircBg,linecolor=\lineColorCircBg,linewidth=\lineWidthCircBg}%
\pscircle(20,20){14}%
% drawPathCircR1
\psset{fillstyle=\fillStyleCircRa,fillcolor=\fillColorCircRa,linestyle=\lineStyleCircRa,linecolor=\lineColorCircRa,linewidth=\lineWidthCircRa}%
\rput(0,0){\psarc(20,20){14}{\angYab}{\angYaa}}%
%
% drawPathLineB1
\newcommand{\lineBa}{\pscustom{%
\psline(1, 16)(1, 14)%
\psbezier(11, 14)(12, 14)(\fpeval{25 + \sqrtA}, 36)%
\psline(\fpeval{25 + \sqrtA}, 36)(25, 36)%
\psbezier(11, 16)(9, 16)(1, 16)%
\closepath}}%
%
\psset{fillstyle=\fillStyleLineBaBg,fillcolor=\fillColorLineBaBg,linestyle=\lineStyleLineBaBg,linecolor=\lineColorLineBaBg,linewidth=\lineWidthLineBaBg,dimen=outer}%
\rput(0,0){\lineBa}%
%
\psset{fillstyle=\fillStyleLineBa,fillcolor=\fillColorLineBa,linestyle=\lineStyleLineBa,linecolor=\lineColorLineBa,linewidth=\lineWidthLineBa,dimen=outer}%
\rput(0,0){\lineBa}%
% drawPathLineY1-background
\psset{fillstyle=\fillStyleLineYaBg,fillcolor=\fillColorLineYaBg,linestyle=\lineStyleLineYaBg,linecolor=\lineColorLineYaBg,linewidth=\lineWidthLineYaBg}%
\rput(20,20){\pscustom{%
\psarc(0,0){14}{\angYbb}{\angYab}%
\psline(14;\angYab)(14;\angYaa)%
\psarc(0,0){14}{\angYaa}{\angYaa}%
\psline(14;\angYba)(14;\angYbb)%
\closepath}}%
% drawPathLineY1
\psset{fillstyle=\fillStyleLineYa,fillcolor=\fillColorLineYa,linestyle=\lineStyleLineYa,linecolor=\lineColorLineYa,linewidth=\lineWidthLineYa}%
\rput(20,20){\pscustom{%
\psarc(0,0){14}{\angYbb}{\angYab}%
\psline(14;\angYab)(14;\angYaa)%
\psarc(0,0){14}{\angYaa}{\angYaa}%
\psline(14;\angYba)(14;\angYbb)%
\closepath}}%
% drawPathLineR1
\psset{fillstyle=\fillStyleLineRa,fillcolor=\fillColorLineRa,linestyle=\lineStyleLineRa,linecolor=\lineColorLineRa,linewidth=\lineWidthLineRa}%
\rput(20,20){\pscustom{%
\psarc(0,0){14}{\angYbb}{\angYab}%
\psline(14;\angYab)(\fpeval{\lineYabx-20},\fpeval{\lineYaby-20})
(\fpeval{\lineYbbx-20},\fpeval{\lineYbby-20})(14;\angYbb)%
\closepath}}%
% drawPathLineG1
\psset{fillstyle=\fillStyleLineGa,fillcolor=\fillColorLineGa,linestyle=\lineStyleLineGa,linecolor=\lineColorLineGa,linewidth=\lineWidthLineGa}%
\rput(20,20){\pscustom{%
\psarcn(0,0){14}{\angYba}{\angYaa}%
\psline(14;\angYaa)(\fpeval{\lineYaax-20},\fpeval{\lineYaay-20})
(\fpeval{\lineYbax-20},\fpeval{\lineYbay-20})(14;\angYba)%
\closepath}}%
% drawPathLineG2
\psset{fillstyle=\fillStyleLineGb,fillcolor=\fillColorLineGb,linestyle=\lineStyleLineGb,linecolor=\lineColorLineGb,
linewidth=\lineWidthLineGb}%
\rput(20,20){\pscustom{%
\psarc(0,0){14}{\angGaa}{\angGba}%
\psline(14;\angGba)(\fpeval{\lineGbbx-20},\fpeval{\lineGbby-20})
(\fpeval{\lineGabx-20},\fpeval{\lineGaby-20})(14;\angGaa)%
\closepath}}%
% drawPathLineG3
\psset{fillstyle=\fillStyleLineGc,fillcolor=\fillColorLineGc,linestyle=\lineStyleLineGc,linecolor=\lineColorLineGc,linewidth=\lineWidthLineGc}%
\rput(20,20){\psarc(0,0){14}{\angGaa}{-30}}%
% drawPathLineR2
\psset{fillstyle=\fillStyleLineRb,fillcolor=\fillColorLineRb,linestyle=\lineStyleLineRb,linecolor=\lineColorLineRb,linewidth=\lineWidthLineRb}%
\rput(20,20){\psarc(0,0){14}{-25}{\fpeval{\angGbb-2}}}%
}}% \psLogoJapanLady
