\kv{fillColorCircRa}{logoRed}%
\kv{lineStyleCircRa}{none}%
\kv{lineColorCircRa}{logoRed}%
\kv{lineWidthCircRa}{0.2}%
\kv{lineDashACircRa}{1}%

% line B1 - drawPathLineB1 - background
\kv{fillStyleLineBaBg}{solid}%
\kv{fillColorLineBaBg}{white}%
\kv{lineStyleLineBaBg}{solid}%
\kv{lineColorLineBaBg}{white}%
