\kv{fillColorLineBa}{logoBlue}%
\kv{lineStyleLineBa}{none}%
\kv{lineColorLineBa}{white}%
\kv{lineWidthLineBa}{0.2}%
\kv{lineDashALineBa}{1}%

% line Y1 - drawPathLineY1 - background
\kv{fillStyleLineYaBg}{solid}%
\kv{fillColorLineYaBg}{logoYellow}%
\kv{lineStyleLineYaBg}{solid}%
\kv{lineColorLineYaBg}{white}%
\kv{lineWidthLineYaBg}{0.8}%
\kv{lineDashALineYaBg}{1}%

% line Y1 - drawPathLineY1
\kv{fillStyleLineYa}{solid}%
\kv{fillColorLineYa}{logoYellow}%
