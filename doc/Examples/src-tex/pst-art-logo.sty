% region Key Value
\def\kvf{pst-art-logo}% key value family
% \input{pst-art-logo-kvp.tex}%

% Default Style
\kv{iconWidth}{20}%
\kv{fillStyle}{solid}%
\kv{fillColor}{white}%
\kv{lineStyle}{none}%
\kv{lineColor}{black}%
\kv{lineWidth}{0.2}%
\kv{lineDashA}{1 1}%

% drawCircleBgM
