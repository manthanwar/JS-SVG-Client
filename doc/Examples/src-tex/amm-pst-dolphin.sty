%region Dolphin A
% \psbezier(2,2)(2,2)(0,11)(9,11)%
% \psbezier(9,11)(9,11)(6,7)(13,11)%
% \psbezier(13,11)(15,12)(17,11)(17,11)%
% \psbezier(17,11)(17,11)(23,8)(18,13)%
% \psbezier(18,13)(20,18)(14,19)(11,18)%
% \psbezier(11,18)(6,22)(3,19)(3,19)%
% \psbezier(3,19)(2,18)(6,19)(4,17)%
% \psbezier(4,17)(-3,9)(2,2)(2,2)%
%-------------------------------------------------------------------------------
\newcommand{\pstDolphinLogoA}[1][]{\setkeys{\kvf}{#1}%
\FPeval{\wide}{round(\iconWidth/20:3)}\psscalebox{\wide}{%
\psset{fillstyle=solid,fillcolor=\bodyColor,linestyle=none}%
% Dolphin
\pscustom{%
\psbezier(2,2)(2,2)(0,11)(9,11)%
(9,11)(6,7)(13,11)%
(15,12)(17,11)(17,11)%
(17,11)(23,8)(18,13)%
(20,18)(14,19)(11,18)%
(6,22)(3,19)(3,19)%
(2,18)(6,19)(4,17)%
(-3,9)(2,2)(2,2)%
}%
% Eye Iris
\psset{fillstyle=solid,fillcolor=\eyeIrisColor,linestyle=none}%
\pscircle(15.5,14.5){0.5}%
% Text
\ifnum\isDolphinText>0\relax%
\rput[lt](0,-1){{\resizebox{20mm}{!}{\fontfamily{cmss}\selectfont\textbf{\color{dolphinBlueD}{DOLPHIN}\color{dolphinBlueL}{STUDIO}}}}}%
\rput[lt](0,-4){{\resizebox{20mm}{!}{\fontfamily{cmss}\selectfont\textbf{\color{dolphinBlueM}{\uppercase{Creating dynamic Designs}}}}}}%
\fi%
% Water
\ifnum\isDolphinWater>0\relax%
\psset{fillstyle=solid,fillcolor=dolphinBlueM,linestyle=none}%
\rput(0,0){\pstDolphinLogoWaterWaveA{20}}%
\rput(9,5){\pstDolphinLogoWaterDropBigA{5}}%
\rput(14,5){\psscalebox{-1 1}{\pstDolphinLogoWaterDropSmallA{2}}}%
\rput(17,3){\psscalebox{-1 1}{\pstDolphinLogoWaterDropSmallA{2}}}%
\psset{fillstyle=solid,fillcolor=dolphinBlueL,linestyle=none}%
\rput(8,5){\pstDolphinLogoWaterDropSmallA{3}}%
\fi%
}}% end \psscalebox % end \pstDolphinLogoWaterWave
%endregion Dolphin A
